{
  "openapi": "3.0.0",
  "info": {
    "title": "SalesAgility REST API",
    "version": "8.1",
    "contact": {
      "name": "Support",
      "url": "https://suitecrm.com/forum"
    },
    "license": {
      "name": "GNU AFFERO GENERAL PUBLIC LICENSE VERSION 3",
      "url": "https://github.com/salesagility/SuiteCRM/blob/master/LICENSE.txt"
    }
  },
  "servers": [{
    "url": "http://localhost:9002/forkedSuite/Api/V8"
  }],
  "paths": {
    "/{module}/{id}": {
      "get": {
        "tags": ["SuiteCRM"],
        "description": "Returns a bean with the specific ID",
        "parameters": [{
          "name": "module",
          "in": "path",
          "description": "Name of the module",
          "required": true,
          "schema": {
            "type": "string"
          },
          "example": "Contacts"
        },
        {
          "name": "id",
          "in": "path",
          "description": "ID of the module",
          "required": true,
          "schema": {
            "type": "string",
            "format": "uuid"
          },
          "example": "b13a39f8-1c24-c5d0-ba0d-5ab123d6e899"
        },
        {
          "name": "fields[module]",
          "in": "query",
          "description": "Fields is for filtering attributes",
          "schema": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "example": "b13a39f8-1c24-c5d0-ba0d-5ab123d6e899"
        }],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/vnd.api+json": {
                "schema": {

                }
              }
            }
          }
        }
      }
    },
    "/logout": {
      "post": {
        "tags": ["SuiteCRM"],
        "description": "Logging out",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/vnd.api+json": {
                "example": {
                  "anyad": "lol"
                }
              }
            }
          },
          "401": {
            "description": "UNAUTHORIZED",
            "content": {
              "application/json": {
                "example": {
                  "anyad": "lol"
                }
              }
            }
          }
        },
        "security": [{
          "oauth2": []
        }]
      }
    }
  },
  "components": {
    "schemas": {
      "meta": {
        "properties": {
          "message": {
            "type": "string",
            "example": "Request was successful"
          }
        }
      },
      "data": {
        "required": [
          "type",
          "id"
        ],
        "properties": {
          "type": {
            "type": "string",
            "example": "Contacts"
          },
          "id": {
            "type": "string",
            "format": "uuid",
            "example": "b13a39f8-1c24-c5d0-ba0d-5ab123d6e899"
          }
        }
      }
    },
    "securitySchemes": {
      "oauth2": {
        "type": "oauth2",
        "flows": {
          "password": {
            "tokenUrl": "http://localhost:9002/forkedSuite/Api/access_token",
            "scopes": {}
          }
        }
      }
    }
  },
  "security": [{
    "oauth2": []
  }]
}